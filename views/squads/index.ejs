<%- include('../partials/header') %>

<!-- TODO re-organise list -->

<table id="squads-list">
    <thead>
      <tr>
        <th>Genre</th>
        <th>Name</th>
        <th>Equipment</th>
        <th>Description</th>
        <th>Units</th>
        <th>User Name</th>
      </tr>
    </thead>
    <tbody>
      <% squads.forEach(function(s) { %>
        <tr>
          <td><%= s.genre %></td>
          <td><%= s.armyName %></td>
          <td><a href="/portfolios"><%= a.squads %></a></td>
          <td><%= s.description %>></td>
          <td><%= s.units %></td>
          <td><%= s.userName %></td>
        </tr> 
      <% }) %>  
    </tbody>
  </table>
  


<%- include('../partials/footer') %>
