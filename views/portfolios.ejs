<%- include('./partials/header') %>
<!-- auto-title in header for this page should just be HammerSpace Protfolios-->


<h1>Portfolios from All Users</h1>
<!-- should this be titles something else? -->

<br>
<a href="/portfolios/new">New</a>
<br>
<br>

<table id="portfolios-list">
  <thead>
    <tr>
      <th>Genre</th>
      <th>Portfolios (name)</th>
      <th>Cover Image</th>
      <th>Armies</th>
      <th>User Name</th>
    </tr>
  </thead>
  <tbody>
    <% portfolios.forEach(function(pf) { %>
      <tr>
        <td><%= pf.genre %></td>
        <td><%= pf.pfName %></td>
        <td>Image example</td>
        <td><%= pf.armies %></td>
        <td><%= pf.user %></td>
      </tr> 
    <% }) %>  
  </tbody>
</table>


<%- include('./partials/footer') %>
