<%- include('./partials/header') %>

<h1>Users</h1>

<table id="user-list">
  <thead>
    <tr>
      <th>User Avatar</th>
      <th>User Name</th>
      <th>Portfolios</th>
      <th>Cover Image</th>
    </tr>
  </thead>
  <tbody>
    <% users.forEach(function(u) { %>
      <tr>
          <td><img src="<%= u.avatar %>"/></td>
          <td><%= u.name %></td>
            <div>
              <td>
                <ul>
                  
                  <li><%= u.portfolios %></li>              
                </ul>
              </td>
            </div>
          <td><img src=""/></td>
      </tr>
    <% }) %>
  </tbody>
</table>


<%- include('./partials/footer') %>
