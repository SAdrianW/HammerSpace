<%- include('./partials/header') %>

<!-- <h1>Users</h1> -->

<table class="table" id="user-list">
  <thead>
    <tr>
      <th>User Avatar</th>
      <th>User Name</th>
      <th>Portfolios</th>
      <th>Cover Image</th>
    </tr>
  </thead>
  <tbody>
    <% users.forEach(function(u) { %>
      <tr>
          <td><img class="img-thumbnail" width="200" height="200" src="<%= u.avatar %>"/></td>
          <td><%= u.name %></td>
            <div>
              <td>
                <ul>
                  <% u.portfolios.forEach((p) => { %>
                  <li>
                    <a href="/portfolios/<%= p._id %>"><%= p.pfName %></a>
                  </li>  <% }) %>            
                </ul>
              </td>
            </div>
          <td><img src=""/></td>
      </tr>
    <% }) %>
  </tbody>
</table>







<%- include('./partials/footer') %>
